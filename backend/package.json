{
  "name": "backend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --poll --respawn --transpile-only --exit-child --no-notify --ignore-watch node_modules -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "build": "npm run build:clean && tsc",
    "build:clean": "npx rimraf dist; exit 0",
    "tsc": "tsc",
    "lint": "eslint src/**",
    "migrate:dev": "prisma migrate dev && prisma generate",
    "db:init": "ts-node-dev -r tsconfig-paths/register src/utils/db_init.ts ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/body-parser": "1.19.5",
    "@types/cookie-parser": "1.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/formidable": "3.4.5",
    "@types/jsonwebtoken": "9.0.5",
    "@types/morgan": "1.9.9",
    "@typescript-eslint/eslint-plugin": "6.18.0",
    "eslint": "8.56.0",
    "eslint-config-standard-with-typescript": "43.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "prisma": "5.8.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@prisma/client": "5.8.1",
    "bcrypt": "5.1.1",
    "body-parser": "1.20.2",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "formidable": "3.5.1",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0"
  }
}
